<% title 'Alta de red' %>

<%= simple_form_for @network, :html => { :class => 'form-horizontal well' } do |f| %>
  <%= f.input :prefix %>
  <%= f.input :name %>
  <%= f.submit 'Crear red', :class => "btn btn-primary" %>
  <%= link_to "Cancelar", "#{networks_path}", :class => "btn btn-sucess" %>
<% end %>

